<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="by.bolbas.model.CoffeeOrder" table="coffeeOrder"
		catalog="coffeeShop" optimistic-lock="version">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="coffeeType" type="string">
			<column name="coffee_type" />
		</property>
		<property name="deliveryType" type="string">
			<column name="delivery_type" />
		</property>
		<property name="weight" type="java.lang.Integer">
			<column name="weight" />
		</property>
		<property name="orderDate" type="date">
			<column name="order_date" />
		</property>
		<property name="startTime" type="time">
			<column name="start_time" />
		</property>
		<property name="endTime" type="time">
			<column name="end_time" />
		</property>
		<property name="totalCost" type="java.lang.Integer">
			<column name="total_cost" />
		</property>
	</class>
</hibernate-mapping>